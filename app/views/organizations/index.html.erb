<% if current_user.organizations.count == 0 %>
	<%= render "new_org" %>
<% end %>


<div class="row full-height">
		<div class="col m3 full-height setlists grey lighten-2">
			<h4 class="center-align"><u>Your Set Lists</u></h4>
			<ul class="collapsible">
				<li class="">Hey</li>
			</ul>
		</div>
		<div class="col m9 full-height">
			<h4 class="center-align">Dashboard Stuff</h4>
			<%= link_to(organization_setlists_path(@current_org), class: "waves-effect waves-light btn-large", method:"post") do %>
				<i class="material-icons left">cloud</i>Create a New Setlist!
				<% end %>
		</div>
</div> 
