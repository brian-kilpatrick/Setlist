<div class="container">
	<div class="row">
		<div class="col s10 offset-s1 form-border">
			<h2><%= @setlist.name %></h2>
			<hr>
			<% @setlist.items.each do |item| %>
			<input type="checkbox" class="filled-in" id="filled-in-box"/>
			<p class="text-header center-align setlist-item"><%= item.arrangement.song.name %></p>
			<% end %>
		</div>
	</div>
</div>